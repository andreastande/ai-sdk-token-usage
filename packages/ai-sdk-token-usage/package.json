{
  "name": "ai-sdk-token-usage",
  "version": "1.0.0",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./metadata": {
      "types": "./dist/metadata/index.d.ts",
      "import": "./dist/metadata/index.mjs",
      "require": "./dist/metadata/index.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "CHANGELOG.md"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch"
  },
  "author": "Andreas Tande",
  "license": "MIT",
  "description": "A lightweight Typescript library to track and visualize token usage across multiple AI model providers.",
  "homepage": "https://ai-sdk-token-usage.vercel.app",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andreastande/ai-sdk-token-usage.git",
    "directory": "packages/streamdown"
  },
  "bugs": {
    "url": "https://github.com/andreastande/ai-sdk-token-usage/issues"
  },
  "keywords": [
    "ai",
    "ai-sdk",
    "tokenlens",
    "llm",
    "chat",
    "token cost",
    "token usage",
    "token tracking",
    "token"
  ],
  "peerDependencies": {
    "ai": ">=5",
    "react": ">=18"
  },
  "devDependencies": {
    "@types/react": "^19.2.0",
    "ai": "^5.0.60",
    "react": "^19.2.0",
    "tsup": "^8.5.0",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "swr": "^2.3.6"
  }
}
